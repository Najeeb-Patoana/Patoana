<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Patoana</title>
    <style>
        * {
            padding: 0;
            margin: 0;
        }
        #div {
            color: goldenrod;
            background-color: #124;
            /* background-color: #3d3d3d; */
            height: 100vh;
            width: 100vw;
            display: flex;
            align-items: center;
            justify-content: center;
        }
    </style>
</head>
<body>
    <div id="div">
        <h1 id="h1">Najeeb</h1>
    </div>
    <script>
        const new_text = "Najeeb Ullah Khan Patoana....";
        let i = 1;
        const text = document.getElementById("h1");
        write();
        function write() {
            text.innerHTML = new_text.slice(0, i);
            i++;
            if (i > new_text.length) {
                i = 1;
            }
            setTimeout(write, 160);
        }
    </script>
</body>
</html>